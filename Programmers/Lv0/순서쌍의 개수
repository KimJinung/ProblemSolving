class Lv1.이상한문자만들기.Solution {
    public int solution(int n) {
        int answer = 0;
        double sqrt = Math.sqrt(n);

        for (int i = 1; i <= sqrt; i++) {
            if (n % i == 0) {
                if (i != sqrt) {
                    answer += 2;
                } else {
                    answer += 1;
                }
            }
        }
        return answer;
    }
}