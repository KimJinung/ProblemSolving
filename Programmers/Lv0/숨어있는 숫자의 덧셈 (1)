class Lv1.이상한문자만들기.Solution {
    public int solution(String my_string) {
        int answer = 0;

        String strNumArr = my_string.replaceAll("[^0-9]", "");

        for (char num : strNumArr.toCharArray()) {
            answer += Character.getNumericValue(num);
        }

        return answer;
    }
}